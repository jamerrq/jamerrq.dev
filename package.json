{
  "name": "jamerrq.dev",
  "version": "0.0.1",
  "dependencies": {
    "@astrojs/check": "^0.3.4",
    "@astrojs/deno": "^5.0.1",
    "@astrojs/preact": "^3.1.0",
    "@astrojs/tailwind": "^5.0.4",
    "@fontsource/merriweather": "^5.0.8",
    "astro": "^4.0.7",
    "astro-critters": "^2.0.11",
    "astro-robots-txt": "^1.0.0",
    "preact": "^10.19.3",
    "simple-icons": "^11.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "deno run --allow-net --allow-read --allow-env ./dist/server/entry.mjs",
    "astro": "astro",
    "lint": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "check-tsx": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "local-build-and-preview": "bun lint && bun check-tsx && bun run build && bun run preview"
  },
  "type": "module",
  "devDependencies": {
    "@fontsource/handlee": "^5.0.18",
    "@fontsource/redressed": "^5.0.18",
    "@fontsource/rubik-doodle-shadow": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.56.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.12.3",
    "sass": "^1.69.7",
    "tailwindcss-debug-screens": "^2.2.1"
  }
}
